@page
@using Microsoft.AspNetCore.Identity
@attribute [IgnoreAntiforgeryToken]
@inject SignInManager<IdentityUser> SignInManager
@functions {
    public async Task<IActionResult> OnPost()
    {
        // IdentityUser user; 
        await SignInManager.SignInAsync(user, );

        return Redirect("~/");
    }
}
